
<h1>Tipo Lesiones</h1>

<table>
  <thead>
    <tr>
      <th>Descripcion</th>
      <th>Estatus</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_lesiones.each do |tipo_lesion| %>
      <tr>
        <td><%= tipo_lesion.descripcion %></td>
        <td><%= tipo_lesion.estatus %></td>
        <td><%= link_to 'Show', tipo_lesion %></td>
        <td><%= link_to 'Edit', edit_tipo_lesion_path(tipo_lesion) %></td>
        <td><%= link_to 'Destroy', tipo_lesion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo Lesion', new_tipo_lesion_path %>
