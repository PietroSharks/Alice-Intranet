
<h1>Tipo Atenciones</h1>

<table>
  <thead>
    <tr>
      <th>Descripcion</th>
      <th>Estatus</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_atenciones.each do |tipo_atencion| %>
      <tr>
        <td><%= tipo_atencion.descripcion %></td>
        <td><%= tipo_atencion.estatus %></td>
        <td><%= link_to 'Show', tipo_atencion %></td>
        <td><%= link_to 'Edit', edit_tipo_atencion_path(tipo_atencion) %></td>
        <td><%= link_to 'Destroy', tipo_atencion, method: :delete, data: { confirm: '¿Está seguro que desea eliminar el tipo de atencion?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo Atencion', new_tipo_atencion_path %>
