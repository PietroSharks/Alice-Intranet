
<h1>Tipo Horarios</h1>

<table>
  <thead>
    <tr>
      <th>Descripcion</th>
      <th>Estatus</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_horarios.each do |tipo_horario| %>
      <tr>
        <td><%= tipo_horario.descripcion %></td>
        <td><%= tipo_horario.estatus %></td>
        <td><%= link_to 'Show', tipo_horario %></td>
        <td><%= link_to 'Edit', edit_tipo_horario_path(tipo_horario) %></td>
        <td><%= link_to 'Destroy', tipo_horario, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo Horario', new_tipo_horario_path %>
