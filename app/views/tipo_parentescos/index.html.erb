
<h1>Tipo Parentescos</h1>

<table>
  <thead>
    <tr>
      <th>Descripcion</th>
      <th>Estatus</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_parentescos.each do |tipo_parentesco| %>
      <tr>
        <td><%= tipo_parentesco.descripcion %></td>
        <td><%= tipo_parentesco.estatus %></td>
        <td><%= link_to 'Show', tipo_parentesco %></td>
        <td><%= link_to 'Edit', edit_tipo_parentesco_path(tipo_parentesco) %></td>
        <td><%= link_to 'Destroy', tipo_parentesco, method: :delete, data: { confirm: '¿Está seguro que desea eliminar el tipo de parentesco?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo Parentesco', new_tipo_parentesco_path %>
