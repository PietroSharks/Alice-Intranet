<p id="notice"><%= notice %></p>

<h1>Busquedas</h1>

<table>
  <thead>
    <tr>
      <th>Descripcion</th>
      <th>Estatus</th>
      <th>Tipo busqueda</th>
      <th>Especialidad</th>
      <th>Especialista</th>
      <th>Tipo servicio</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @busquedas.each do |busqueda| %>
      <tr>
        <td><%= busqueda.descripcion %></td>
        <td><%= busqueda.estatus %></td>
        <td><%= busqueda.tipo_busqueda %></td>
        <td><%= busqueda.especialidad %></td>
        <td><%= busqueda.especialista %></td>
        <td><%= busqueda.tipo_servicio %></td>
        <td><%= link_to 'Show', busqueda %></td>
        <td><%= link_to 'Edit', edit_busqueda_path(busqueda) %></td>
        <td><%= link_to 'Destroy', busqueda, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Busqueda', new_busqueda_path %>
