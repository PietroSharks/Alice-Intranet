
<h1>Tipo Calificaciones</h1>

<table>
  <thead>
    <tr>
      <th>Descripcion</th>
      <th>Estatus</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_calificaciones.each do |tipo_calificacion| %>
      <tr>
        <td><%= tipo_calificacion.descripcion %></td>
        <td><%= tipo_calificacion.estatus %></td>
        <td><%= link_to 'Show', tipo_calificacion %></td>
        <td><%= link_to 'Edit', edit_tipo_calificacion_path(tipo_calificacion) %></td>
        <td><%= link_to 'Destroy', tipo_calificacion, method: :delete, data: { confirm: '¿Está seguro que desea eliminar el tipo de calificacion?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo Calificacion', new_tipo_calificacion_path %>
