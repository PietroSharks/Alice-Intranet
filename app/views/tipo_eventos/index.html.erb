
<h1>Tipo Eventos</h1>

<table>
  <thead>
    <tr>
      <th>Descripcion</th>
      <th>Estatus</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_eventos.each do |tipo_evento| %>
      <tr>
        <td><%= tipo_evento.descripcion %></td>
        <td><%= tipo_evento.estatus %></td>
        <td><%= link_to 'Show', tipo_evento %></td>
        <td><%= link_to 'Edit', edit_tipo_evento_path(tipo_evento) %></td>
        <td><%= link_to 'Destroy', tipo_evento, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo Evento', new_tipo_evento_path %>
