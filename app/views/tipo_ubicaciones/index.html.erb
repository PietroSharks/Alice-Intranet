<p id="notice"><%= notice %></p>

<h1>Tipo Ubicaciones</h1>

<table>
  <thead>
    <tr>
      <th>Descripcion</th>
      <th>Estatus</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_ubicaciones.each do |tipo_ubicacion| %>
      <tr>
        <td><%= tipo_ubicacion.descripcion %></td>
        <td><%= tipo_ubicacion.estatus %></td>
        <td><%= link_to 'Show', tipo_ubicacion %></td>
        <td><%= link_to 'Edit', edit_tipo_ubicacion_path(tipo_ubicacion) %></td>
        <td><%= link_to 'Destroy', tipo_ubicacion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo Ubicacion', new_tipo_ubicacion_path %>
