
<h1>Tipo Eventualidades</h1>

<table>
  <thead>
    <tr>
      <th>Descripcion</th>
      <th>Estatus</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_eventualidades.each do |tipo_eventualidad| %>
      <tr>
        <td><%= tipo_eventualidad.descripcion %></td>
        <td><%= tipo_eventualidad.estatus %></td>
        <td><%= link_to 'Show', tipo_eventualidad %></td>
        <td><%= link_to 'Edit', edit_tipo_eventualidad_path(tipo_eventualidad) %></td>
        <td><%= link_to 'Destroy', tipo_eventualidad, method: :delete, data: { confirm: '¿Está seguro que desea eliminar el tipo de eventualidad?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo Eventualidad', new_tipo_eventualidad_path %>
