
<h1>Tipo Notificaciones</h1>

<table>
  <thead>
    <tr>
      <th>Descripcion</th>
      <th>Estatus</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_notificaciones.each do |tipo_notificacion| %>
      <tr>
        <td><%= tipo_notificacion.descripcion %></td>
        <td><%= tipo_notificacion.estatus %></td>
        <td><%= link_to 'Show', tipo_notificacion %></td>
        <td><%= link_to 'Edit', edit_tipo_notificacion_path(tipo_notificacion) %></td>
        <td><%= link_to 'Destroy', tipo_notificacion, method: :delete, data: { confirm: '¿Está seguro que desea eliminar el tipo de notificacion?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo Notificacion', new_tipo_notificacion_path %>
